{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "airbnb", 
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["__filename", "__dirname"] }],
    "@typescript-eslint/no-unused-vars": 2,
    "max-len": [2, 80],
    "max-params": [2, 3]

    }
}